cat > .env.example << 'EOF'
# OpenAI API Configuration
VITE_OPENAI_API_KEY=sk-your_openai_api_key_here

# App Configuration
VITE_APP_ENV=development
VITE_DUMMY_MODE=false

# API Endpoints
VITE_API_BASE_URL=http://localhost:8000
EOF

# 실제 .env 파일 생성 (개발용)
cp .env.example .env
# 여기서 실제 OpenAI API 키 입력해야 